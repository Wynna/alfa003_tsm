void main()
{
    ACR_TriggerOnExit();

    // Custom code goes here.
	
	//Forthpeak Exterior quest script.
 if(GetTag(OBJECT_SELF) == "fp_scout_02_b")
 	{object oCorpse = GetObjectByTag("fp_scout_01_orccorpse");
	 SetUseableFlag(oCorpse, FALSE);
	 }

 if(GetTag(OBJECT_SELF) == "colddamage_trig")
	{SetLocalInt(GetEnteringObject(), "iToggle", 1);
	 DelayCommand(35.0, SetLocalInt(GetEnteringObject(), "iToggle", 0));
	}
		   

}